require 'selenium-webdriver'
require 'logger'

class JupiterToysTest
    def initialize(driver)
        @driver = driver
        @logger = Logger.new(STDOUT)
        
        @driver.manage.timeouts.implicit_wait = 3
        @driver.manage.window.maximize
    end

    def getDriver
        @driver
    end

    def endDriver
        @driver.quit
    end
end